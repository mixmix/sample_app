<li>
  <%= gravatar_for user, size: 52 %>
  <%= link_to user.name, user %>
  <% if user.can_be_deleted_by?(current_user) #current_user.admin? && !current_user?(user) %>
    | <%= link_to "delete", user, method: :delete, data: { confirm: "You sure?" } %> <!-- | is a literal character on page-->
  <% end %>
</li>

